{
  "name": "react-use-bireducer",
  "author": "soywod <clement.douin@posteo.net>",
  "description": "React hook for managing reducers and effects together.",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/react-use-bireducer.esm.js",
  "size-limit": [
    {
      "path": "dist/react-use-bireducer.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-use-bireducer.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "husky": "^8.0.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "typescript-language-server": "^0.10.1"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  }
}
